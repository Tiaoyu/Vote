@{
    var voteList = ViewData["VOTE_LIST"] as List<RoundViewModel>;
}

<div class="row">
    @foreach (var target in voteList)
    {
    <div name="target" class="col-xs-4 col-sm-3" style="vertical-align:middle">
        <input type="hidden" name="targetId" value="@target.TargetId" />
        <img width="150" src="~/@target.TargetContent" /><br />
        <label>Votes:</label><label>@target.TargetValue</label>
    </div>
    }

</div>